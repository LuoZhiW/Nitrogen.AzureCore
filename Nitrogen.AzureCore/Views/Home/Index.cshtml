@{
    // ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin</title>
    <script src="@Url.Content("~/scripts/jquery/jquery-1.10.2.min.js")"></script>
    <script src="@Url.Content("~/scripts/plugins/cookie/jquery.cookie.js")"></script>
    <link href="@Url.Content("~/styles/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/scripts/bootstrap/bootstrap.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/scripts/bootstrap/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/scripts/plugins/dialog/dialog.js")"></script>
    <script src="@Url.Content("~/nitrogenjs/nitrogen-clientdata.js")"></script>
    <link href="@Url.Content("~/styles/nitrogen-ui.css")" rel="stylesheet" />
    <script src="@Url.Content("~/nitrogenjs/nitrogen-ui.js")"></script>
    <script src="@Url.Content("~/admin/index.js")"></script>
    <link href="@Url.Content("~/admin/css/index.css")" rel="stylesheet" />
    <link href="@Url.Content("~/admin/css/skins/_all-skins.css")" rel="stylesheet" />
    <link href="@Url.Content("~/styles/nitrogen-im.css")" rel="stylesheet" />
</head>
<body class="hold-transition skin-blue sidebar-mini" style="overflow: hidden;">
    <div id="ajax-loader" style="cursor: progress; position: fixed; top: -50%; left: -50%; width: 200%; height: 200%; background: #fff; z-index: 10000; overflow: hidden;">
        <img src="~/content/images/ajax-loader.gif" style="position: absolute; top: 0; left: 0; right: 0; bottom: 0; margin: auto;" />
    </div>
    <div class="wrapper">
        <!--头部信息-->
        <header class="main-header">
            <a href="#" class="logo">
                <span class="logo-mini">NG</span>
                <span class="logo-lg"><strong>Nitrogen.CRM</strong></span>
            </a>
            <nav class="navbar navbar-static-top">
                <a class="sidebar-toggle">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown messages-menu">
                            <a class="mess" href="#" class="dropdown-toggle" data-toggle="dropdown">
                                @*  <i class="fa fa-envelope-o "></i>*@
                                <span class="label label-success"></span>
                            </a>
                        </li>
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/images/head/user2-160x160.png" class="user-image" alt="User Image">
                                <span class="hidden-xs">administrator</span>
                            </a>
                            <ul class="dropdown-menu pull-right">
                                <li><a class="menuItem" data-id="userInfo" href="/PersonCenter/Index"><i class="fa fa-user"></i>个人信息</a></li>
                                <li><a href="javascript:void();"><i class="fa fa-trash-o"></i>清空缓存</a></li>
                                <li class="divider"></li>
                                <li><a href="javascript:void();" onclick="$.learunindex.indexOut()"><i class="ace-icon fa fa-power-off"></i>安全退出</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!--左边导航-->
        <div class="main-sidebar">
            <div class="sidebar">
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/content/images/head/user2-160x160.png" class="img-circle" alt="User Image" />

                    </div>
                    <div class="pull-left info">
                        <p>Admin</p>
                        <a><i class="fa fa-circle text-success"></i>在线</a>
                    </div>
                </div>
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Search...">
                        <span class="input-group-btn">
                            <a class="btn btn-flat"><i class="fa fa-search"></i></a>
                        </span>
                    </div>
                </form>
                <ul class="sidebar-menu" id="sidebar-menu">
                    <li class="header">导航菜单</li>
                </ul>
            </div>
        </div>
        <!--中间内容-->
        <div id="content-wrapper" class="content-wrapper">
            <div class="content-tabs">
                <button class="roll-nav roll-left tabLeft">
                    <i class="fa fa-backward"></i>
                </button>
                <nav class="page-tabs menuTabs">
                    <div class="page-tabs-content" style="margin-left: 0px;">
                        <a href="javascript:;" class="menuTab active" data-id="/Home/AdminLTEDesktop">欢迎首页</a>
                    </div>
                </nav>
                <button class="roll-nav roll-right tabRight">
                    <i class="fa fa-forward" style="margin-left: 3px;"></i>
                </button>
                <div class="btn-group roll-nav roll-right">
                    <button class="dropdown tabClose" data-toggle="dropdown">
                        页签操作<i class="fa fa-caret-down" style="padding-left: 3px;"></i>
                    </button>
                    <ul class="dropdown-menu dropdown-menu-right">
                        <li><a class="tabReload" href="javascript:void();">刷新当前</a></li>
                        <li><a class="tabCloseCurrent" href="javascript:void();">关闭当前</a></li>
                        <li><a class="tabCloseAll" href="javascript:void();">全部关闭</a></li>
                        <li><a class="tabCloseOther" href="javascript:void();">除此之外全部关闭</a></li>
                    </ul>
                </div>
                <button class="roll-nav roll-right fullscreen"><i class="fa fa-arrows-alt"></i></button>
            </div>
            <div class="content-iframe" style="overflow: hidden;">
                <div class="mainContent" id="content-main">
                    <iframe class="LRADMS_iframe" width="100%" height="100%" src="javascript:void(0)" frameborder="0" data-id="/Home/AdminLTEDesktop"></iframe>
                </div>
            </div>
        </div>
    </div>
    <div id="loading_background" class="loading_background" style="display: none;"></div>
    <div id="loading_manage">
        正在拼了命为您加载…
    </div>
    <div class="message-wrap">
        <div class="message-content">
            <div class="message-header">
                企业内部微信
                <div class="message-close">
                    <a href="javascript:;">×</a>
                </div>
            </div>
            <div class="message-body">
                <div class="message-body-search">
                    <input type="text" class="search-text" placeholder="搜索：同事名称、讨论组名称">
                    <i class="fa fa-search"></i>
                </div>
                <div class="message-body-container">
                    <div class="message-nav" style="display: block;">
                        <ul>
                            <li id="nav-contact-tab" class="active">
                                <a title="联系人"><i class="fa fa-user"></i></a>
                            </li>
                            <li id="nav-group-tab">
                                <a title="讨论组"><i class="fa fa-users" style="font-size: 20px;"></i></a>
                            </li>
                            <li id="nav-last-tab">
                                <a title="最近回话"><i class="fa fa-comment"></i><span class="message-count">2</span></a>
                            </li>
                        </ul>
                    </div>
                    <div class="message-group" style="height: 335px; overflow: auto; overflow-x: hidden;">
                        <ul id="message-contact-list"></ul>
                        <ul id="message-group-list" class="message-chatlist" style="display: none;">
                            <div style="color: red;">目前不支持</div>
                        </ul>
                        <ul id="message-last-list" class="message-chatlist" style="display: none;">
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>